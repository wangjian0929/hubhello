    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/tranparent_background"
        android:id="@+id/monthcalendar"
        >

    <RelativeLayout
        android:id="@+id/custommonthview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"
        android:background="@color/white">
        <RelativeLayout
            android:id="@+id/allmonthlayout"
            android:background="@color/white"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_centerHorizontal="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/monthbackward"
                        android:rotation="180"
                        android:background="@drawable/nexticon"
                        android:layout_width="@dimen/_8sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:layout_gravity="center"
                        android:gravity="center_horizontal"/>
                    <TextView
                        android:id="@+id/textmonth"
                        android:hint="February 2019"
                        android:textSize="@dimen/_15sdp"
                        android:layout_marginStart="@dimen/_30sdp"
                        android:layout_marginEnd="@dimen/_30sdp"
                        android:textColorHint="@color/month_grey"
                        android:textColor="@color/month_grey"
                        android:layout_gravity="center"
                        android:gravity="center_horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/avenirltstd_book"/>
                    <ImageView
                        android:id="@+id/monthforward"
                        android:background="@drawable/nexticon"
                        android:layout_width="@dimen/_8sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:gravity="center_horizontal"/>
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
        <LinearLayout
            android:layout_below="@+id/allmonthlayout"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginBottom="@dimen/_20sdp">
            <LinearLayout
                android:weightSum="4"
                android:layout_marginTop="@dimen/_8sdp"
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <Button
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:text="Jan"
                    android:id="@+id/btnjan"
                    android:tag="1"
                    android:textAllCaps="false"
                    android:textAlignment="center"
                    android:textColor="@color/month_grey"
                    android:textSize="@dimen/_13sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_50sdp"
                    android:fontFamily="@font/avenirltstd_book"
                    android:focusable="true"/>

                <Button
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:text="Feb"
                    android:id="@+id/btnfeb"
                    android:tag="1"
                    android:textAllCaps="false"
                    android:textAlignment="center"
                    android:textColor="@color/month_grey"
                    android:textSize="@dimen/_13sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_50sdp"
                    android:fontFamily="@font/avenirltstd_book"
                    android:focusable="true"/>

