<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/groupLayout">
    <RelativeLayout
        android:id="@+id/dateLauout"
        android:layout_centerVertical="true"
        android:layout_width="@dimen/_70sdp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_2sdp">
        <RelativeLayout
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_marginEnd="@dimen/_5sdp"
            android:layout_centerVertical="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/layoutCurrent"
                android:layout_centerHorizontal="true"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:background="@drawable/background_highlight"
                android:layout_marginBottom="@dimen/_8sdp"
                android:visibility="visible">
            </RelativeLayout>

            <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                android:id="@+id/textDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_5sdp"
                android:hint="26"
                android:text=""
                android:textColor="@color/button_color"
                android:textColorHint="@color/white"
                android:textSize="@dimen/_18sdp" />
            <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                android:id="@+id/textDay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textDate"
                android:layout_marginTop="@dimen/_3sdp"
                android:layout_centerHorizontal="true"
                android:text=""
                android:hint=""
                android:textColor="@color/view_color"
                android:textSize="@dimen/_10sdp" />
            <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                android:textColor="@color/view_color"
                android:textSize="@dimen/_10sdp"
                android:layout_centerHorizontal="true"
                android:text=""
                android:hint=""
                android:id="@+id/texMonth"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_below="@+id/textDay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/recyclerlayout"
        android:layout_marginStart="@dimen/_2sdp"
        android:layout_toEndOf="@+id/dateLauout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <!--android:layout_marginTop="@dimen/_3sdp"-->
        <!--android:layout_marginBottom="@dimen/_3sdp"-->
        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerviewEating"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        </android.support.v7.widget.RecyclerView>
    </RelativeLayout>
    <View
        android:id="@+id/Vieww"
        android:layout_below="@+id/recyclerlayout"
        android:background="@color/view_color"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
</RelativeLayout>
















