<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/Description"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:filterTouchesWhenObscured="true"
    android:focusableInTouchMode="true">
    <RelativeLayout
        android:id="@+id/headertitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/sick_green">
        <RelativeLayout
            android:id="@+id/ViewLayout"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_margin="@dimen/_10sdp"
                android:layout_centerVertical="true"
                android:id="@+id/Back"
                android:background="@drawable/backbutton"
                android:backgroundTint="@color/white"
                android:layout_width="@dimen/_10sdp"
                android:layout_height="@dimen/_15sdp"/>
            <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                android:textSize="@dimen/_13sdp"
                android:layout_toEndOf="@+id/Back"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/_minus5sdp"
                android:textColor="@color/white"
                android:text="Menu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RelativeLayout>
    </RelativeLayout>
    <android.support.v4.widget.NestedScrollView
        android:id="@+id/ScrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_1sdp">
        <LinearLayout
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ParseData">
                <RelativeLayout
                    android:id="@+id/Header"
                    android:layout_margin="@dimen/_5sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.feedAustralia.android.LanguageFont.FontTextView_Bold
                        android:text="Morning Shake"
                        android:textSize="@dimen/_14sdp"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:id="@+id/MenuName"
                        android:layout_centerVertical="true"
                        android:textColor="@color/sick_green"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/WholeLayout"
                    android:layout_below="@+id/Header"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10sdp">
                    <RelativeLayout
                        android:id="@+id/ImageLayout"
                        android:background="@drawable/image_border"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_width="@dimen/_71sdp"
                        android:layout_height="@dimen/_71sdp">
                        <ImageView
                            android:id="@+id/roundedImageView"
                            android:layout_width="@dimen/_70sdp"
                            android:layout_height="@dimen/_70sdp"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_marginBottom="@dimen/_2sdp"
                        android:layout_marginTop="@dimen/_minus2sdp"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_toEndOf="@+id/ImageLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                            android:text=""
                            android:textSize="@dimen/_13sdp"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:hint="Fruit Platter"
                            android:id="@+id/menuItemName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <RelativeLayout
                            android:id="@+id/FirstLayout"
                            android:layout_below="@+id/menuItemName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <RelativeLayout
                                android:id="@+id/First"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:background="@drawable/text_bordergreen"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:visibility="gone">
                                <com.feedAustralia.android.LanguageFont.FontTextView_Regular
                                    android:id="@+id/Fruit"
                                    android:hint=""
                                    android:padding="@dimen/_2sdp"
                                    android:textColorHint="@color/white"
                                    android:layout_marginStart="@dimen/_3sdp"
                                    android:layout_centerVertical="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/_9sdp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <RelativeLayout
                                    android:layout_marginStart="@dimen/_10sdp"
                                    android:layout_marginEnd="@dimen/_5sdp"
                                    android:layout_toEndOf="@+id/Fruit"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:layout_marginBottom="@dimen/_2sdp"
                                    android:background="@drawable/rating_border"
                                    android:id="@+id/ratingtext"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                                        android:text=""
                                        android:hint=""
                                        android:layout_marginStart="@dimen/_3sdp"
                                        android:layout_marginEnd="@dimen/_3sdp"
                                        android:layout_centerVertical="true"
                                        android:textSize="@dimen/_8sdp"
                                        android:textColor="@color/black"
                                        android:textColorHint="@color/black"
                                        android:id="@+id/FruitValue"
                                        android:padding="@dimen/_2sdp"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"/>
                                </RelativeLayout>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_toEndOf="@+id/First"
                                android:id="@+id/Second"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:background="@drawable/dairytext_border"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:visibility="gone">
                                <com.feedAustralia.android.LanguageFont.FontTextView_Regular
                                    android:id="@+id/Dairy"
                                    android:hint=""
                                    android:padding="@dimen/_2sdp"
                                    android:textColorHint="@color/white"
                                    android:layout_marginStart="@dimen/_3sdp"
                                    android:layout_centerVertical="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/_9sdp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                                <RelativeLayout
                                    android:layout_marginStart="@dimen/_10sdp"
                                    android:layout_marginEnd="@dimen/_5sdp"
                                    android:layout_toEndOf="@+id/Dairy"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:layout_marginBottom="@dimen/_2sdp"
                                    android:background="@drawable/rating_border"
                                    android:id="@+id/ratingtex3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                                        android:text=""
                                        android:hint=""
                                        android:layout_marginStart="@dimen/_3sdp"
                                        android:layout_marginEnd="@dimen/_3sdp"
                                        android:layout_centerVertical="true"
                                        android:textSize="@dimen/_8sdp"
                                        android:textColor="@color/black"
                                        android:textColorHint="@color/black"
                                        android:id="@+id/DairyValue"
                                        android:padding="@dimen/_2sdp"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"/>
                                </RelativeLayout>
                            </RelativeLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_marginTop="@dimen/_3sdp"
                            android:layout_below="@+id/FirstLayout"
                            android:id="@+id/Third"
                            android:background="@drawable/vegtext_border"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:visibility="gone">
                            <com.feedAustralia.android.LanguageFont.FontTextView_Regular
                                android:id="@+id/Bread"
                                android:hint=""
                                android:padding="@dimen/_2sdp"
                                android:textColorHint="@color/white"
                                android:layout_marginStart="@dimen/_3sdp"
                                android:layout_centerVertical="true"
                                android:textColor="@color/white"
                                android:textSize="@dimen/_9sdp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <RelativeLayout
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:layout_toEndOf="@+id/Bread"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:layout_marginBottom="@dimen/_2sdp"
                                android:background="@drawable/rating_border"
                                android:id="@+id/ratingtext1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                                    android:text=""
                                    android:hint=""
                                    android:layout_marginStart="@dimen/_3sdp"
                                    android:layout_marginEnd="@dimen/_3sdp"
                                    android:layout_centerVertical="true"
                                    android:textSize="@dimen/_8sdp"
                                    android:textColor="@color/black"
                                    android:id="@+id/BreadValue"
                                    android:textColorHint="@color/black"
                                    android:padding="@dimen/_2sdp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                            </RelativeLayout>
                        </RelativeLayout>
                        <RelativeLayout
                            android:id="@+id/Forth"
                            android:layout_marginTop="@dimen/_3sdp"
                            android:layout_below="@+id/Third"
                            android:background="@drawable/meat_text_border"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:visibility="gone">
                            <com.feedAustralia.android.LanguageFont.FontTextView_Regular
                                android:id="@+id/Meat"
                                android:hint=""
                                android:padding="@dimen/_2sdp"
                                android:textColorHint="@color/white"
                                android:layout_marginStart="@dimen/_3sdp"
                                android:layout_centerVertical="true"
                                android:textColor="@color/white"
                                android:textSize="@dimen/_9sdp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <RelativeLayout
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:layout_toEndOf="@+id/Meat"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:layout_marginBottom="@dimen/_2sdp"
                                android:background="@drawable/rating_border"
                                android:id="@+id/ratingtextfour"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                                    android:text=""
                                    android:hint=""
                                    android:layout_marginStart="@dimen/_3sdp"
                                    android:layout_marginEnd="@dimen/_3sdp"
                                    android:layout_centerVertical="true"
                                    android:textSize="@dimen/_8sdp"
                                    android:textColor="@color/black"
                                    android:id="@+id/MeatValue"
                                    android:textColorHint="@color/black"
                                    android:padding="@dimen/_2sdp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                            </RelativeLayout>
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_marginTop="@dimen/_3sdp"
                            android:layout_below="@+id/Forth"
                            android:background="@drawable/vegetables_text_border"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/Fifth"
                            android:visibility="gone">
                            <com.feedAustralia.android.LanguageFont.FontTextView_Regular
                                android:id="@+id/VegetablesData"
                                android:hint=""
                                android:padding="@dimen/_2sdp"
                                android:textColorHint="@color/white"
                                android:layout_marginStart="@dimen/_3sdp"
                                android:layout_centerVertical="true"
                                android:textColor="@color/white"
                                android:textSize="@dimen/_9sdp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <RelativeLayout
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:layout_toEndOf="@+id/VegetablesData"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:layout_marginBottom="@dimen/_2sdp"
                                android:background="@drawable/rating_border"
                                android:id="@+id/ratingtextfive"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">
                                <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                                    android:text=""
                                    android:hint=""
                                    android:layout_marginStart="@dimen/_3sdp"
                                    android:layout_marginEnd="@dimen/_3sdp"
                                    android:layout_centerVertical="true"
                                    android:textSize="@dimen/_8sdp"
                                    android:textColor="@color/black"
                                    android:id="@+id/VegetablesValue"
                                    android:textColorHint="@color/black"
                                    android:padding="@dimen/_2sdp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"/>
                            </RelativeLayout>
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
                <View
                    android:layout_below="@+id/WholeLayout"
                    android:background="@color/view_color"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"/>
            </RelativeLayout>

            <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                android:id="@+id/noteDetails"
                android:text="Note this is a childcare service recipe designed to serve 40 children"
                android:textSize="@dimen/_13sdp"
                android:textColor="@color/black"
                android:padding="@dimen/_2sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:lineSpacingExtra="@dimen/_2sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:layout_marginTop="@dimen/_10sdp"
                android:background="@color/border_color"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>
            <com.feedAustralia.android.LanguageFont.FontTextView_Bold
                android:id="@+id/Ingredients"
                android:text="Ingredients"
                android:textSize="@dimen/_14sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:textColor="@color/sick_green"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                android:id="@+id/ingradientTV"
                android:text="@string/description"
                android:textSize="@dimen/_13sdp"
                android:textColor="@color/black"
                android:padding="@dimen/_2sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"

                android:lineSpacingExtra="@dimen/_2sdp"
                android:textAlignment="textStart"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:layout_marginTop="@dimen/_1sdp"
                android:background="@color/view_color"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>
            <com.feedAustralia.android.LanguageFont.FontTextView_Bold
                android:padding="@dimen/_5sdp"
                android:text="Nutritional facts (per serving)"
                android:textSize="@dimen/_14sdp"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:textColor="@color/sick_green"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_3sdp"
                android:orientation="horizontal"
                android:background="@color/item_color"
                android:weightSum="3">
                <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                    android:layout_weight="1.5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"/>
                <RelativeLayout
                    android:layout_weight="0.80"
                    android:textAlignment="viewEnd"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">
                    <com.feedAustralia.android.LanguageFont.FontTextView_Bold
                        android:id="@+id/Text1"
                        android:layout_width="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:text="@string/per_serve_per_child"
                        android:textColor="@color/sick_green"
                        android:textSize="@dimen/_13sdp"
                        android:layout_height="@dimen/_17sdp"/>
                    <com.feedAustralia.android.LanguageFont.FontTextView_Bold
                        android:layout_below="@+id/Text1"
                        android:layout_width="wrap_content"
                        android:textAlignment="viewEnd"
                        android:layout_alignParentEnd="true"
                        android:text="@string/per_child"
                        android:layout_marginEnd="@dimen/_1sdp"
                        android:textColor="@color/sick_green"
                        android:textSize="@dimen/_13sdp"
                        android:layout_height="@dimen/_16sdp"/>
                </RelativeLayout>
                <com.feedAustralia.android.LanguageFont.FontTextView_Bold
                    android:layout_weight="0.75"
                    android:layout_width="0dp"
                    android:textAlignment="center"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:text="Per 100g"
                    android:textColor="@color/sick_green"
                    android:textSize="@dimen/_13sdp"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <android.support.v7.widget.RecyclerView
                android:layout_marginTop="@dimen/_8sdp"
                android:id="@+id/recyclerView4"
                android:layout_width="match_parent"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:layout_height="wrap_content">
            </android.support.v7.widget.RecyclerView>
            <View
                android:layout_marginTop="@dimen/_8sdp"
                android:background="@color/view_color"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>
            <com.feedAustralia.android.LanguageFont.FontTextView_Bold
                android:padding="@dimen/_5sdp"
                android:text="Method"
                android:textSize="@dimen/_14sdp"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:textColor="@color/sick_green"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <com.feedAustralia.android.LanguageFont.FontTextView_SemiBold
                android:id="@+id/Method"
                android:text=""
                android:textSize="@dimen/_13sdp"
                android:textColor="@color/black"
                android:padding="@dimen/_2sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:lineSpacingExtra="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</LinearLayout>







